<form action="/posts/new" method="post">

  <fieldset>
      <legend>Post</legend>

      <label for="title">Title</label>
      <input type="text" name="title" value="<%= @post.title%>">

      <label for="url">Url</label>
      <input type="text" name="url">

      <input type="submit" value="create_post">
  </fieldset>


</form>
